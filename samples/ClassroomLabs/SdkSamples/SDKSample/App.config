<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <appSettings>
    <!-- ========================================== -->
    <!-- App registration details -->
    <!-- ========================================== -->    
    <add key="ClientId" value="[Enter the Client Id (Application ID obtained from the Azure portal), e.g. ba74781c2-53c2-442a-97c2-3d60re42f403]" />
    <add key="ClientSecret" value="[Enter the Client Secret (Application secret) added from the Certificates and secrets section for the application in the Azure portal]" />
    <add key="TenantId" value="[Enter the Tenant Id (Directory ID obtained from the Azure portal), e.g. da41245a5-11b3-996c-00a8-4d99re19f292]" />
    <!-- ========================================== -->
    <!-- Sample workflow details -->
    <!-- ========================================== -->
    <add key="SubscriptionId" value="[Enter the Subscription Id that contains the Lab Account and the Application registration], e.g. ba74781c2-53c2-442a-97c2-3d60re42f401" />
    <add key="ResourceGroupName" value="[Enter the name of the resource group that the Lab Accoun is in], e.g. lab-account-rg" />
    <add key="LabAccountName" value="[Enter the name of the Lab Account]" />
    <add key="LabName" value="[Enter the name of the lab]" />
    <add key="OutputPath" value="[Enter the folder path to output the RDP files to - make sure the folder exists and that you escape backslashes], e.g. C:\\rdpfiles" />
  </appSettings>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
  </startup>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-10.0.0.0" newVersion="10.0.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>
